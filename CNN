      "source": [
        "\r\n",
        "from keras.datasets import mnist\r\n",
        "(X1_train, Y1_train), (X2_test, Y2_test) = mnist.load_data()\r\n",
        "\r\n",
        "Train_images = X1_train[:2000]\r\n",
        "Train_labels = Y1_train[:2000]\r\n",
        "Test_images = X2_test[:2000]\r\n",
        "Test_labels = Y2_test[:2000]\r\n",
        "conv = conv_op(8, 3)\r\n",
        "pool = max_pool(2)\r\n",
        "sm = Softmax(13 * 13 *8, 10)\r\n"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "2o83n0ZgIovB",
        "outputId": "e595a476-4cce-44c8-f5a8-160863fe8da8"
      },
      "source": [
        "def cnn_forward_prop(image, label):\r\n",
        "  out_p = conv.forward_prop((image / 255)-0.5)\r\n",
        "  out_p = pool.forward_prop(out_p)\r\n",
        "  out_p = sm.forward_prop(out_p)\r\n",
        "\r\n",
        "  cross_ent_loss = -np.log(out_p[label])\r\n",
        "  accuracy_eval = 1 if np.argmax(out_p) == label else 0\r\n",
        "\r\n",
        "  return out_p, cross_ent_loss, accuracy_eval\r\n",
        "\r\n",
        "\r\n",
        "\r\n",
        "\r\n",
        "def training_cnn(image, label, learn_rate = 0.005):\r\n",
        "\r\n",
        "  #forward\r\n",
        "  out, loss, acc = cnn_forward_prop(image, label)\r\n",
        "  \r\n",
        "  #calculate initial gradient\r\n",
        "  gradient = np.zeros(10)\r\n",
        "  gradient[label] = -1 / out[label]\r\n",
        "\r\n",
        "  #backprop\r\n",
        "  grad_back = sm.back_prop(gradient, learn_rate)\r\n",
        "  grad_back = pool.back_prop(grad_back)\r\n",
        "  grad_back = conv.back_prop(grad_back, learn_rate)\r\n",
        "\r\n",
        "  return loss,acc\r\n",
        "\r\n",
        "for epoch1 in range(4):\r\n",
        "  print('Epoch %d ---->' % (epoch1 + 1))\r\n",
        "\r\n",
        "  #shuffle the train data\r\n",
        "  shuffle_data = np.random.permutation(len(Train_images))\r\n",
        "  train_images = Train_images[shuffle_data]\r\n",
        "  train_labels = Train_labels[shuffle_data]\r\n",
        "\r\n",
        "  #training the CNN\r\n",
        "  loss =0 \r\n",
        "  num_correct = 0\r\n",
        "  for i, (im , label) in enumerate(zip(Train_images, Train_labels)):\r\n",
        "    if i % 100 == 0:\r\n",
        "      print('%d steps out of 100: average loss %.3f and accuraxy: %d%%' %(i + 1, loss / 100, num_correct))\r\n",
        "      loss = 0\r\n",
        "      num_correct =  0\r\n",
        "    l1, accu = training_cnn(im, label)\r\n",
        "    loss += l1\r\n",
        "    num_correct +=  accu\r\n",
        "  \r\n",
        "  #test the cnn\r\n",
        "print('**Model test')\r\n",
        "loss = 0\r\n",
        "num_correct = 0\r\n",
        "for im, label in zip(Test_images,Test_labels):\r\n",
        "  _, l1, accu = cnn_forward_prop(im,label)\r\n",
        "  loss += l1\r\n",
        "  num_correct += accu\r\n",
        "  tests = len(Test_images)\r\n",
        "print('Test loss:',loss/tests)\r\n",
        "print('Test Accuracy', (num_correct/tests))"
      ],
      "execution_count": null,
